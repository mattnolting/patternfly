<{{#if check--IsStandalone}}label{{else if check--type}}{{check--type}}{{else}}div{{/if}} class="{{pfv}}check
  {{#if check--IsStandalone}}
    pf-m-standalone
  {{/if}}
  {{#if check--modifier}}
    {{check--modifier}}
  {{/if}}"
  {{#if check--id}}
    id="{{check--id}}"
  {{/if}}
  {{#if check--attribute}}
    {{{check--attribute}}}
  {{/if}}>
  {{> @partial-block
    check-input--IsDisabled=check--IsDisabled
    check-label--IsDisabled=check--IsDisabled
  }}
</{{#if check--IsStandalone}}label{{else if check--type}}{{check--type}}{{else}}div{{/if}}>
