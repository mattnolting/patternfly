{{#> table-tr table-tr--attribute='role="row"' table-tr--tree--HasActions=table-tr--HasActions}}
  {{#> table-th table-th--modifier=(pfPrefix 'table__tree-view-title-cell')}}
    {{#> table-tree-view-main}}
      {{#> config table-th--type='span' table-td--type='span'}}
        {{#unless (concat table-tr--tree--IsLeaf table-tr--HasNoPosinset table--HasNoPosinset)}}
          {{> table-cell-toggle button--id=(concat table--id '-expandable-toggle-' table-tr--tree--index)}}
        {{/unless}}
        {{#unless table-tr--tree--HasNoCheckbox}}
          {{#if table-tree-view--HasCheckboxes}}
            {{> table-cell-check}}
          {{/if}}
        {{/unless}}
        {{#if table-tr--tree--HasActions}}
          {{> table-cell-action action--id=(concat table--id '-' table-tr--tree--index '-actions')}}
        {{/if}}
      {{/config}}
      {{#> table-tree-view-text}}
        {{#if table-tree-view--HasIcons}}
          {{#> table-tree-view-icon}}
            {{#if table-tr--tree--IsLeaf}}
              <i class="fas fa-fw fa-leaf" aria-hidden="true"></i>
            {{else}}
              <i class="fas fa-fw fa-folder" aria-hidden="true"></i>
            {{/if}}
          {{/table-tree-view-icon}}
        {{/if}}
        {{#> table-text table-text--attribute=(concat table--id '-node-' table-tr--tree--index)}}
          {{#if table-tr--tree--title}}
            {{{table-tr--tree--title}}}
          {{else}}
            Datacenters
          {{/if}}
        {{/table-text}}
      {{/table-tree-view-text}}
      {{> table-tree-view-details-toggle}}
      {{#if @partial-block}}
        {{> @partial-block}}
      {{/if}}
    {{/table-tree-view-main}}
  {{/table-th}}
  {{#> table-td table-td--data-label="Migration assessment"}}
    10
  {{/table-td}}
  {{#> table-td table-td--data-label="Size of VM"}}
    25
  {{/table-td}}
  {{#> table-td table-td--data-label="Number of Data Stores"}}
    5
  {{/table-td}}
  {{> table-cell-action action--id=(concat table--id '-' table-tr--tree--index '-actions')}}
{{/table-tr}}
