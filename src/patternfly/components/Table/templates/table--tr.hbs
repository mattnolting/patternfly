{{#if table--tr--IsExpandable}}
  {{#> table-tr table-tr--expandable="true"}}
    {{#if table--HasToggles}}
      {{> table-td table-td--IsEmpty="true"}}
    {{/if}}
    {{#if table--HasChecks}}
      {{> table-td table-td--IsEmpty="true"}}
    {{/if}}
    {{#> table-td table-td--attribute=(concat 'colspan="' table--tr--content-cols '" id="' table--id '-content' table-tr--index '"')}}
      {{#> table-expandable-row-content}}
        {{> @partial-block}}
      {{/table-expandable-row-content}}
    {{/table-td}}
    {{#if table--HasActions}}
      {{> table-td table-td--IsEmpty="true"}}
    {{/if}}
  {{/table-tr}}
{{else}}
  {{#> table-tr}}
    {{#if table--HasToggles}}
      {{> table--toggle}}
    {{/if}}
    {{#if table--HasChecks}}
      {{> table--check}}
    {{/if}}
    {{> @partial-block}}
    {{#if table--HasActions}}
      {{> menu-toggle menu-toggle--IsPlain=true menu-toggle--HasKebab=true menu-toggle--aria-label='Actions'}}
    {{/if}}
  {{/table-tr}}
{{/if}}
