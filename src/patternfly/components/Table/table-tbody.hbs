{{#>
  config
  IsTbody=true
}}

<tbody class="{{pfv}}table__tbody
  {{setModifiers
    table-tbody--IsClickable='pf-m-clickable'
    table-tbody--IsSelected='pf-m-selected'
    table-tbody--IsExpanded='pf-m-expanded'
    table-tbody--modifier=table-tbody--modifier
  }}"
  {{#if table--IsGrid}}
    role="rowgroup"
  {{/if}}
  {{#if table-tbody--IsClickable}}
    tabindex="0"
  {{/if}}
  {{#if table-tbody--attribute}}
    {{{table-tbody--attribute}}}
  {{/if}}>
  {{#if @partial-block}}
    {{> @partial-block}}
  {{/if}}
</tbody>

{{/config}}
