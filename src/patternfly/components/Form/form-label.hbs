<{{#if form-group--IsCheckGroup}}span{{else}}label{{/if}} class="pf-c-form__label{{#if form-label--modifier}} {{form-label--modifier}}{{/if}}"
  {{#if form-label--attribute}}
    {{{form-label--attribute}}}
  {{/if}}>
  {{#> form-label-text}}
    {{>@partial-block}}
  {{/form-label-text}}
  {{#if required}}
    &nbsp;{{> form-label-required}}
  {{/if}}
  {{#if form-label--HasHelpIcon}}
    &nbsp;{{> form-group-label-help form-group-label-help--aria-label="More information for name field" form-group-label-help--aria-describedby=(concat form--id form-group--id)}}
  {{/if}}
</{{#if form-group--IsCheckGroup}}span{{else}}label{{/if}}>
