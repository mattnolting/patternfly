{{#> table table--id="table-loading-table" table--IsGrid=true table--modifier="pf-m-grid-xl" table--aria-label='This is a table showing a loading state"'}}
  {{#> table-thead}}
    {{#> table-tr}}
      {{#> table-td table-td--IsCheck=true}}
        {{#> check check--modifier="pf-m-standalone"}}
          {{#> check-input check-input--attribute='name="check-all" aria-label="Select all rows"'}}{{/check-input}}
        {{/check}}
      {{/table-td}}
      {{#> table-th}}
        Repositories
      {{/table-th}}
      {{#> table-th}}
        Branches
      {{/table-th}}
      {{#> table-th}}
        Pull requests
      {{/table-th}}
      {{#> table-th}}
        Workspaces
      {{/table-th}}
      {{#> table-th}}
        Last commit
      {{/table-th}}
      {{> table-cell table-cell--IsEmpty=true}}
      {{> table-cell table-cell--IsEmpty=true}}
    {{/table-tr}}
  {{/table-thead}}

  {{#> table-tbody}}
    {{#> table-tr}}
      {{#> table-td table-td--attribute='colspan="8"'}}
          {{#> empty-state empty-state--modifier="pf-m-sm"}}
            {{#> empty-state-icon}}
              {{#> spinner}}Loading...{{/spinner}}
            {{/empty-state-icon}}
            {{#> title titleType="h2" title--modifier="pf-m-lg"}}
              Loading
            {{/title}}
          {{/empty-state}}
      {{/table-td}}
    {{/table-tr}}
  {{/table-tbody}}
{{/table}}
