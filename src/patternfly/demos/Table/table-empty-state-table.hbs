{{#> table table--id="empty-state-table-demo" table--IsGrid=true table--modifier="pf-m-grid-xl" table--aria-label='This is a table showing an empty state"'}}
  {{#> table-thead}}
    {{#> table-tr}}
      {{#> table-td table-td--IsCheck=true}}
        {{#> check check--modifier="pf-m-standalone"}}
          {{#> check-input check-input--attribute='name="check-all" aria-label="Select all rows"'}}{{/check-input}}
        {{/check}}
      {{/table-td}}
      {{#> table-th}}
        Repositories
      {{/table-th}}
      {{#> table-th}}
        Branches
      {{/table-th}}
      {{#> table-th}}
        Pull requests
      {{/table-th}}
      {{#> table-th}}
        Workspaces
      {{/table-th}}
      {{#> table-th}}
        Last commit
      {{/table-th}}
      {{> table-cell table-cell--IsEmpty=true}}
      {{> table-cell table-cell--IsEmpty=true}}
    {{/table-tr}}
  {{/table-thead}}

  {{#> table-tbody}}
    {{#> table-tr}}
      {{#> table-td table-td--attribute='colspan="8"'}}
          {{#> empty-state empty-state--modifier="pf-m-sm"}}
            {{#> empty-state-icon empty-state-icon--type="search"}}{{/empty-state-icon}}
            {{#> title titleType="h2" title--modifier="pf-m-lg"}}
              No results found
            {{/title}}
            {{#> empty-state-body}}
              No results match the filter criteria. Remove all filters or clear all filters to show results.
            {{/empty-state-body}}
            {{#> empty-state-primary}}
              {{#> button button--modifier="pf-m-link"}}
                Clear all filters
              {{/button}}
            {{/empty-state-primary}}
          {{/empty-state}}
      {{/table-td}}
    {{/table-tr}}
  {{/table-tbody}}
{{/table}}
