{{#> page-sidebar page-sidebar--ExcludeSidebarBody=true}}
  {{#if page-template-sidebar--nav--IsExpandable}}
    {{#> page-sidebar-body}}
      {{> page-template-expandable-nav}}
    {{/page-sidebar-body}}
  {{else if page-template-sidebar--nav--IsGrouped}}
    {{#> page-sidebar-body}}
      {{> page-template-grouped-nav}}
    {{/page-sidebar-body}}
  {{else}}
    {{#> page-sidebar-body}}
      {{> page-template-sidebar-nav}}
    {{/page-sidebar-body}}
  {{/if}}
{{/page-sidebar}}
